#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
   int arr[]={1,2,4,5,6};
   int val=3;int pos=2;int n=5;
   for(int i=n;i>pos;i--)// inserting the element at specific positon of array;
       arr[i]=arr[i-1];
    arr[pos]=val;
    n++;
   
   for(int i=0;i<n;i++)
       cout<<arr[i]<<" ";
   

    
}
