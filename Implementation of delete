#include <stdio.h>
#include <iostream>
using namespace std;

int main()
{
   int arr[]={1,2,4,5,6};
   int pos=2;int n=5;
   for(int i=pos;i<n-1;i++) // removing the element at specific positon of array; 
       arr[i]=arr[i+1];
    n--;
   
   for(int i=0;i<n;i++)
       cout<<arr[i]<<" ";
   

    
}
